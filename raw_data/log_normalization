        whoLetDogsOut.computeIfAbsent("snoop", k -&gt; f(k));
        whoLetDogsOut.computeIfAbsent("snoop", k -&gt; f(k));
    }
    static boolean f(String s) {
        System.out.println("creating a value for \""+s+'"');
        return s.isEmpty();
    }
}
