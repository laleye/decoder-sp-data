private static long gcd(long a, long b)
{
    while (b &gt; 0)
    {
        long temp = b;
        b = a % b; // % is remainder
        a = temp;
    }
    return a;
}

private static long gcd(long[] input)
{
    long result = input[0];
    for(int i = 1; i &lt; input.length; i++) result = gcd(result, input[i]);
    return result;
}
