    Map&lt;K,Boolean&gt; seen = new ConcurrentHashMap&lt;&gt;();
    Function&lt;T,K&gt; keyExtractor;
    public DistinctByKey(Function&lt;T,K&gt; ke) {
        this.keyExtractor = ke;
    }
    public boolean filter(T t) {
