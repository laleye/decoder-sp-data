private fun &lt;T&gt; anyObject(): T {
    Mockito.anyObject&lt;T&gt;()
    return uninitialized()
}

private fun &lt;T&gt; uninitialized(): T = null as T

@Test
fun myTest() {
    `when`(mockedBackend).login(anyObject())).thenAnswer { ... }
}
