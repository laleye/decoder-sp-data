public &lt;T&gt; List&lt;T&gt; magicalListGetter(Class&lt;T&gt; klazz) {
    List&lt;T&gt; list = new ArrayList&lt;&gt;();
    list.add(klazz.cast(actuallyT));
    try {
        list.add(klazz.getConstructor().newInstance()); // If default constructor
    } ...
    return list;
}
