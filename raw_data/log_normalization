public class GenericList &lt;T&gt; extends ArrayList&lt;T&gt;
{
     private Class&lt;T&gt; genericType;

     public GenericList(Class&lt;T&gt; c)
     {
          this.genericType = c;
     }

     public Class&lt;T&gt; getGenericType()
     {
          return genericType;
     }
}
