            return map;
        } else {
            return map;
        }
    }

    /**
     * Copied from WebDataBinder.
     * 
     * @param multipartFiles
     * @param mpvs
     */
    protected void bindMultipart(Map&lt;String, List&lt;MultipartFile&gt;&gt; multipartFiles, MutablePropertyValues mpvs) {
